{
  "name": "Blizzcon Autofiller",
  "description": "Autofill blizzcon order form",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {"scripts":["background.js"]},
  "content_scripts": [
      {"js": ["content.js"], "matches": ["<all_urls>"], "all_frames":true}
  ],
  "browser_action": {
    "default_title": "Blizzcon Autofiller"
  },
  "manifest_version": 2
}